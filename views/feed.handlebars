<h1>Feed</h1>
{{#if error}}
  <span>{{error}}</span>
{{else}}
  <ul class="list-group">
    {{#each feed}}
    <li class="list-group-item">{{this}}</li>
    {{/each}}
  </ul>
{{/if}}
<form class="form-inline" method="POST" action="/user/feed">
  <div class="form-group">
    <select class="form-control" id="id" name="id">
      <option>1</option>
      <option>2</option>
      <option>3</option>
    </select>
  </div>
  <input type="submit" class="btn btn-primary" value="View">
</form>
